#include<stdio.h>
#include<stdlib.h>
#include<math.h>
void main()
{ 
int a, b, c;
int ch;
printf("\nEnter two numbers\n");
scanf("%d%d", &a, &b);
printf("\n\n");
printf("\n 1-ADD \n");
printf("\n 2-SUB \n");
printf("\n 3-MULTI \n");
printf("\n 4-DIVIDE \n");
printf("\n 5-MODULO \n");
printf("\nEnter your choice of operation:\n");
scanf("%d", &ch);
switch(ch)
{
case 1: c=a+b;
break; 
case 2: c=a-b;
break;
case 3: c=a*b;
break;
case 4: if(b==0)
{
printf("\nDivide by zero error!\n");
exit(0);
}
else 
{
c=(float)a/b;
break;
}
case 5: c=a%b;
break;
default: printf("\nInvalid choice!\n");
exit(0);
}
printf("\nThe result is:%d\n",c);
} 




#include<stdio.h>
int main()
{
int i, j;
for(i=1;i<=5;i++)
{
for(j=1;j<=10;j++)
{
printf("%d\t",i*j);
} 
printf("\n\n");
}
}
